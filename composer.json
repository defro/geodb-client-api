{
  "name": "fgalvao/geodb-client-api",
  "description": "PHP client for GeoDB Cities API",
  "homepage": "https://github.com/ffgalvao/geodb-client-api",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Felippe Ladeira",
      "email": "ffgalvao@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "../BaseClientApi",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "fgalvao/base-client-api": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "fGalvao\\GeoDB\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "fGalvao\\GeoDB\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
